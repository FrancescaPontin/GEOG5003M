        "\n",
        "# calcualte the centorid\n",
        "# and replace the leeds geometry (polygon) with the centroid geometry (point)\n",
        "leeds_centroid['geometry'] = leeds.centroid\n",
        "\n",
        "# check - geometry should contain point data\n",
        "leeds_centroid.head()"
      ]
