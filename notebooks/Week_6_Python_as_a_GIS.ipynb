      "metadata": {
        "id": "Q8FVxDlbRmOk"
      },
      "outputs": [],
      "source": [
        "#Import the required packages\n",
        "\n",
        "import pandas as pd\n",
        "import numpy as np\n",
        "import geopandas as gpd\n",
        "import matplotlib.pyplot as plt\n",
        "import pyproj\n",
        "import contextily as ctx\n",
        "import seaborn as sns\n",
        "\n",
